path: /tests/client-dbus
duration: 20m
tag: python

require:
  - clevis-luks
  - make
  - python3-dbus
  - python3-dbus-client-gen
  - python3-dbus-python-client-gen
  - python3-hypothesis
  - python3-psutil
  - python3-pyudev
  - python3-tenacity

environment:
  TANG_URL: localhost
  RUST_LOG: stratisd=debug
  STRATISD: /usr/libexec/stratisd
  STRATIS_DUMPMETADATA: /usr/bin/stratis-dumpmetadata
  PYTHONPATH: ./src

/v2/udev:
  summary: Run Python model tests
  test: make -f Makefile model-tests
